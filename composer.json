{
    "name": "laravel-notification-channels/twitter",
    "description": "This package makes it easy to send notifications via Twitter status updates with Laravel 5.3",
    "homepage": "https://github.com/laravel-notification-channels/twitter",
    "license": "MIT",
    "authors": [
        {
            "name": "Christoph Rumpel",
            "email": "c.rumpel@kabsi.at",
            "homepage": "http://christoph-rumpel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "abraham/twitteroauth": "^0.6.4",
        "illuminate/notifications": "5.3.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*",
        "orchestra/testbench": "3.3.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Twitter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Twitter\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
